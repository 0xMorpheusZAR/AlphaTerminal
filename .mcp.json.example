{
  "mcpServers": {
    "coingecko_mcp_local": {
      "command": "npx",
      "args": [
        "-y",
        "@coingecko/coingecko-mcp"
      ],
      "env": {
        "COINGECKO_PRO_API_KEY": "your_coingecko_pro_api_key_here",
        "COINGECKO_ENVIRONMENT": "pro"
      }
    },
    "@21st-dev/magic": {
      "command": "npx",
      "args": ["-y", "@21st-dev/magic@latest"],
      "env": {
        "API_KEY": "your_magic_api_key_here"
      }
    }
  }
}